{{#each feeders}}
<h4>
    Name: {{ name }}
</h4>
<h4>
    Last Seen: {{ lastSeen }}
</h4>
<h4>
    Last Fed: {{ lastFeeding }}
</h4>
<h4>Owner ID: {{ owner }}</h4>
<table>
    <tr>
        <th>
            Time
        </th>
        <th>
            Amount
        </th>
    </tr>
    {{#each schedules}}
    <tr>
        <td>
            {{hour}}:{{minute}}
        </td>
        <td>
            {{cups}} cups
        </td>
    </tr>
    {{/each}}
</table>
<form method="POST" action="/admin/feeders/{{_id}}/claim">
    <select name="userId">
        {{#each ../users}}
        <option value="{{_id}}">
            {{username}}
        </option>
        {{/each}}
    </select>
    <input type="submit" value="Submit">
        Assign to user
    </input>
</form>
<form method="POST" action="/admin/feeders/{{_id}}/delete">
    <input type="submit" value="Delete">
        Delete Feeder
    </input>
</form>
<hr>
{{/each}}

